;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="2D Mining"
run/main_scene="res://game/startup.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://game/items/pickaxe/pickaxe.png"

[autoload]

NodeDebugger="*res://game/components/debug component/node_debugger.gd"
Global="*res://game/autoload singletons/global.gd"
DebugHud="*res://game/ui/debug_hud.tscn"
DataManager="*res://game/autoload singletons/data_manager.tscn"
Effects="*res://game/effects/effects.tscn"
SoundPlayer="*res://game/audio/sound_player.tscn"
GameManager="*res://game/autoload singletons/game_manager.tscn"
UserConfig="*res://game/autoload singletons/user_config.gd"
PositionalSoundPlayer="*res://game/audio/positional_sound_player.tscn"
AdminPanel="*res://game/ui/admin/admin_panel.tscn"

[editor]

movie_writer/movie_file="C:/_CODING/godot/2D-Mining/gameplay.avi"
movie_writer/fps=30

[editor_plugins]

enabled=PackedStringArray("res://addons/sound_material_matrix/plugin.cfg")

[file_customization]

folder_colors={
"res://game/": "yellow",
"res://game/blocks/": "orange",
"res://game/items/": "teal",
"res://media/": "gray"
}

[filesystem]

import/blender/enabled=false

